import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;
import java.util.Arrays;
import java.util.HashSet;

public class HashSet2 {
	public static void main(String[] args)
	{
		long time1,time2,time3,time4;
		try 
		{
			time1 = System.nanoTime();
			String line;
			BufferedReader reader = new BufferedReader(new FileReader("Alice.txt"));
			HashSet<String> book = new HashSet<>();
			//loops through until there isn't a line left to read
			while((line = reader.readLine())!= null)
			{
				//splits the line into an array and the adds the whole array to the hashset
				String[] words = line.split(" ");
				book.addAll(Arrays.asList(words));
			}
			//loops through the hashset and prints the each element 
			for(String word : book)
			{
				System.out.println(word);
			}
			time2 = System.nanoTime();
			reader.close();
			long mil1 = (long) ((time2 - time1)/1e6);
			System.out.println("Elapsed time for loading hash set: " + mil1 + "milliseconds");
			time3 = System.nanoTime();
			for(int i = 0; i<100;i++)
			{
				book.contains("Cheshire");
			}
			time4 = System.nanoTime();
			long mil2 = (long) ((time4 - time3)/1e6);
			System.out.println("Elapsed time for searching hash set: " + mil2 + "milliseconds");
		}
		catch(IOException e)
		{
			System.out.println("Something went wrong.");
		}
	}
}
